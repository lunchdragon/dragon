<?xml version="1.0" encoding="windows-1252"?>

<securityContext>
    <loginPage>login.jsf</loginPage>
    <loginPageHTML>login-html.jsf</loginPageHTML>
    <securePaths>
         <securePath path="/rest/*">
            <authType>Basic</authType>
        </securePath>
        <securePath path="/ws/*">
            <authType>Form</authType>
        </securePath>
        <securePath path="/html/*">
            <authType>HTML</authType>
        </securePath>
        <securePath path="/rn/*">
            <authType>HTML</authType>
        </securePath>
    </securePaths>
</securityContext>